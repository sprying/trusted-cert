{
  "name": "trusted-cert",
  "version": "1.0.0",
  "description": "A simple tool to generate self signed x509 certificate",
  "bin": {
    "trusted-cert": "./cli.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "rm -rf dist types && rollup -c",
    "watch": "rm -rf dist types && rollup -c -w",
    "release": "cross-env NODE_ENV=production rollup -c && npm publish"
  },
  "dependencies": {
    "command-exists": "^1.2.9",
    "commander": "^5.0.0",
    "debug": "^4.3.1",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "moment": "^2.29.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "rollup": "^2.45.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "standardx": "^7.0.0",
    "typescript": "^4.2.4"
  },
  "author": "sprying<sprying.fang@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/sprying/trusted-cert/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sprying/trusted-cert"
  },
  "bugs": {
    "url": "https://github.com/sprying/trusted-cert/issues"
  },
  "keywords": [
    "ssl",
    "openssl",
    "certificate",
    "trust",
    "https",
    "trust store",
    "x509",
    "certificates",
    "self-signed",
    "self-signing",
    "credential",
    "credentials",
    "keychain",
    "ca"
  ],
  "files": [
    "cli.js",
    "dist/*",
    "types/*"
  ],
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
